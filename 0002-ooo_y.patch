+public class TestFor{
+	public static void main(String args[]){
+		double s = 83;
+		int r = 45 ;
+		String str = new String();
+		str = fuction1(s);
+		str = fuction2(s);
+		str = fuction3(s);
+		str = fuction4(s);
+		str = fuction5(r);
+		System.out.println("运行结束！");
+		}
+
+	/**
+	for 循环练习
+	*/
+	public static String fuction1(double score){
+		System.out.println("for 循环练习 ：");
+		int sum = 0 ;
+		for(int i = 0 ; i < 5; i++){
+			 sum += i ; 
+			 System.out.println("i = "+i+" ; sum = "+sum);
+			}
+		return null;
+}
+
+	
+	/**
+	while 循环练习
+	*/
+	public static String fuction2(double score){
+		System.out.println("while 循环练习 ：");
+		int sum = 0 ;
+		int i = 0;
+		while(i<5){
+			sum += i ; 
+			System.out.println("i = "+i+" ; sum = "+sum);
+			i++;
+			}
+		return null;
+}
+
+	
+	/**
+	do_while 循环练习
+	*/
+	public static String fuction3(double score){
+		System.out.println("do_while 循环练习 ：");
+		int sum = 0 ;
+		int i = 0;
+		do{
+			sum += i ; 
+			System.out.println("i = "+i+" ; sum = "+sum);
+			i++;
+			}while(i<5);
+		return null;
+}
+	/**
+	素数打印 练习
+	*/
+	public static String fuction4(double score){
+		System.out.println("素数打印 练习 ：");
+		int sum = 100 ;
+		int i = 0;
+		do{
+			i=2;
+			boolean b = false;
+			do{
+				if(0==sum%i){
+					b = true;
+					}
+				i++;
+				}while(i<sum/2);
+				if(!b){
+				System.out.println(sum + " 为素数 ；");
+				}
+			sum++;
+			}while(sum<200);
+		return null;
+}
+	/**
+	打花练习
+	*/
+	public static String fuction5(int r){
+		//System.out.println("素数打印 练习 ：");
+		int i = 1;
+		for (;i<r;i++){
+		int j = 1;
+			for(;j<r-i;j++){
+				System.out.print(" ");
+			}
+			j=1;
+			for(;j<=i;j++){
+				System.out.print("* ");
+			}
+			//System.out.print("")
+			System.out.println("");
+			}
+			return null;
+}
+
+}
